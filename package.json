    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "@types/passport": "^1.0.11",
    "@types/passport-local": "^1.0.34",
    "@types/request": "^2.48.8",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "cssnano": "5.1.14",
