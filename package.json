    "cookie-session": "^2.0.0",
    "ejs": "^3.1.8",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "lucide": "^0.101.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.7",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
